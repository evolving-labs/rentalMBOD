		<version>0.0.9</version>
	</parent>
	<groupId>com.telespree.apps</groupId>
	<artifactId>rentalMBOD</artifactId>
	<version>2.3.0-SNAPSHOT</version>
	<packaging>war</packaging>
	<name>Hotspot Subscriber Portal Server</name>
	<description>Hotspot Subscriber Portal Server</description>
	<properties>
		<izpack.standalone.compiler.version>4.3.4</izpack.standalone.compiler.version>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
	</properties>
	<scm>
		<connection>scm:perforce://depot/ASClients/rentalMBOD</connection>
	</scm>
	<build>
		<resources>
			<resource>
				<directory>src/main/resources</directory>
			</resource>
			<resource>
				<targetPath>${project.build.directory}/izpack</targetPath>
				<filtering>true</filtering>
				<directory>src/izpack</directory>
				<excludes>
					<exclude>install.xml</exclude>
				</excludes>
			</resource>
		</resources>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<configuration>
					<source>1.6</source>
					<target>1.6</target>
					<encoding>UTF-8</encoding>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-release-plugin</artifactId>
				<configuration>
					<connectionUrl>scm:perforce://depot/ASClients/rentalMBOD</connectionUrl>
					<useReleaseProfile>false</useReleaseProfile>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-dependency-plugin</artifactId>
				<executions>
					<execution>
						<id>izpack</id>
						<phase>verify</phase>
						<goals>
							<goal>copy</goal>
						</goals>
						<configuration>
							<artifactItems>
								<artifactItem>
									<groupId>com.telespree.common</groupId>
									<artifactId>installer</artifactId>
									<version>1.0.2</version>
									<type>jar</type>
									<overWrite>true</overWrite>
									<outputDirectory>${project.build.directory}/izpack</outputDirectory>
									<destFileName>installer.jar</destFileName>
								</artifactItem>
								<artifactItem>
									<groupId>oracle</groupId>
									<artifactId>ojdbc14</artifactId>
									<version>10.2.0.4.0</version>
									<type>jar</type>
									<overWrite>true</overWrite>
									<outputDirectory>${project.build.directory}/izpack</outputDirectory>
									<destFileName>oracle.jar</destFileName>
								</artifactItem>
							</artifactItems>
						</configuration>
					</execution>
				</executions>
			</plugin>
			<plugin>
				<groupId>org.codehaus.izpack</groupId>
				<artifactId>izpack-maven-plugin</artifactId>
				<version>1.0-alpha-5</version>
				<executions>
					<execution>
						<phase>verify</phase>
						<goals>
							<goal>izpack</goal>
						</goals>
						<configuration>
						</configuration>
					</execution>
				</executions>
				<dependencies>
					<dependency>
						<groupId>org.codehaus.izpack</groupId>
						<artifactId>izpack-standalone-compiler</artifactId>
						<version>${izpack.standalone.compiler.version}</version>
					</dependency>
				</dependencies>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-assembly-plugin</artifactId>
				<executions>
					<execution>
						<id>assembly-installer</id>
						<phase>package</phase>
						<goals>
							<goal>single</goal>
						</goals>
						<configuration>
							<descriptors>
								<descriptor>
									assembly-installer.xml</descriptor>
							</descriptors>
							<outputDirectory>${project.build.directory}/izpack</outputDirectory>
						</configuration>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>
